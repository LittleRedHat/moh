<script type="text/javascript">
//<![CDATA[
function jsOpenSite(formEl) {	
	var errorMessage = null;
	var objFocus = null;
	if (formEl.sitelink.value.length == 0) {
		errorMessage = "please! select a site to move!";
		objFocus = formEl.sitelink;
	} 
	if(errorMessage != null) {
		alert(errorMessage);
		objFocus.focus();
		return false;
	}
	return true;
}
